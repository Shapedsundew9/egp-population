{
    "created": {
        "check_with": "valid_created",
        "coerce": "datetime_str_to_datetime",
        "meta": {
            "description": "The UTC date and time of the populations definition creation."
        },
        "type": "datetime"
    },
    "creator": {
        "coerce": "uuid_str_to_uuid",
        "meta": {
            "description": "The UUID of the creator of the problem."
        },
        "nullable": true,
        "type": "uuid"
    },
    "description": {
        "default": null,
        "maxlength": 16384,
        "meta": {
            "description": "User defined arbitary string. Not used by Erasmus."
        },
        "minlength": 0,
        "nullable": true,
        "regex": "[ -~]{0,16384}",
        "type": "string"
    },
    "egp_problem": {
        "default": "./",
        "maxlength": 128,
        "meta": {
            "description": "Either a git hash of one of the EGP problem definitions or a file path to a problem."
        },
        "minlength": 1,
        "nullable": false,
        "regex": "([0-9a-f]{40}|[0-9a-f]{64}|[ -~]{1,128}/)",
        "type": "string"
    },
    "inputs": {
        "maxlength": 256,
        "meta": {
            "description": "Input interface of every individual in the population."
        },
        "minlength": 0,
        "schema": {
            "check_with": "valid_ep_type",
            "max": 256,
            "min": 1,
            "type": "string"
        },
        "type": "list"
    },
    "meta_data": {
        "default": null,
        "maxlength": 16384,
        "meta": {
            "description": "User defined arbitary string. Not used by Erasmus."
        },
        "minlength": 0,
        "nullable": true,
        "regex": "[ -~]{0,16384}",
        "type": "string"
    },
    "name": {
        "maxlength": 64,
        "meta": {
            "description": "User defined name for the population. Must be unique in the Gene Pool."
        },
        "minlength": 1,
        "nullable": false,
        "required": true,
        "regex": "[A-Za-z_][\\w]{0,63}",
        "type": "string"
    },
    "ordered_interface_hash": {
        "max": 9223372036854775807,
        "min": -9223372036854775808,
        "type": "integer"
    },
    "outputs": {
        "maxlength": 256,
        "meta": {
            "description": "Output interface of every individual in the population."
        },
        "minlength": 0,
        "schema": {
            "check_with": "valid_ep_type",
            "max": 256,
            "min": 1,
            "type": "string"
        },
        "type": "list"
    },
    "size": {
        "default": 128,
        "max": 2147483647,
        "meta": {
            "description": "The size of the population to be created / evolved in an epoch. Usually this would be 100's may be 10's, maybe 1000's."
        },
        "min": 1,
        "type": "integer"
    },
    "survivability": {
        "default": "elitist",
        "maxlength": 256,
        "meta": {
            "description": "Either a python module path with a 'survivability_function.py' file in it or the name of a standard survivability function. End paths with '/' to avoid the possibility of name conflicts."
        },
        "minlength": 0,
        "regex": "[ -~]{0,256}",
        "type": "string"
    },
    "uid": {
        "max": 32767,
        "meta": {
            "description": "Population UID is assigned by the Gene Pool if it does not already exist."
        },
        "min": 0,
        "type": "integer"
    },
    "unordered_interface_hash": {
        "max": 9223372036854775807,
        "min": -9223372036854775808,
        "type": "integer"
    },
    "updated": {
        "check_with": "valid_updated",
        "coerce": "datetime_str_to_datetime",
        "meta": {
            "description": "The UTC date and time user defined fields not used by Erasmus were updated."
        },
        "type": "datetime"
    },
    "worker_id": {
        "coerce": "uuid_str_to_uuid",
        "meta": {
            "description": "The worker that created this population."
        },
        "type": "uuid"
    }
}